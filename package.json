{
  "name": "airtable-bridge",
  "version": "1.0.0",
  "description": "Middleware platform for Airtable API limitations",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "ts-node prisma/seed.ts",
    "docker:build": "docker build -t airtable-bridge .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "airtable": "^0.12.0",
    "aws-sdk": "^2.1400.0",
    "bullmq": "^4.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.0",
    "express-rate-limit": "^6.8.0",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.0",
    "node-cron": "^3.0.0",
    "prisma": "^5.0.0",
    "uuid": "^9.0.0",
    "winston": "^3.10.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.0",
    "jest": "^29.0.0",
    "nodemon": "^3.0.0",
    "supertest": "^6.3.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "airtable",
    "api",
    "middleware",
    "rate-limiting",
    "batching"
  ],
  "author": "Airtable Bridge Team",
  "license": "MIT"
}
